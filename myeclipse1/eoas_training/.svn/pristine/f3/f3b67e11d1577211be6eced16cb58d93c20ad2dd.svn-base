<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="article" extends="default" namespace="/article">
		<action name="toList" class="articleAction" method="toList">
			<result>/jsp/oa/article/list.jsp</result>
		</action>
		
		<action name="list" class="articleAction" method="list">
			<result type="json"><param name="root">jsonMap</param></result>
		</action>
		<action name="toAdd" class="articleAction" method="toAdd">
			<result>/jsp/oa/article/add.jsp</result>
		</action>
		
		<action name="add" class="articleAction" method="add">
			<result>/jsp/oa/article/add.jsp</result>
		</action>

	</package>
</struts>