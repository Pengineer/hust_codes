package csdc.bean;

import java.util.Set;

public class SystemOption {
	private String id;
	private String name;
	private String description;
	private SystemOption systemOption;
	private String code;
	private Boolean isAvailable;
	private String standard;
	private String abbr;
	
	private Set<SystemOption> systemOptions;
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public SystemOption getSystemOption() {
		return systemOption;
	}
	public void setSystemOption(SystemOption systemOption) {
		this.systemOption = systemOption;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public Boolean getIsAvailable() {
		return isAvailable;
	}
	public void setIsAvailable(Boolean isAvailable) {
		this.isAvailable = isAvailable;
	}
	public String getStandard() {
		return standard;
	}
	public void setStandard(String standard) {
		this.standard = standard;
	}
	public String getAbbr() {
		return abbr;
	}
	public void setAbbr(String abbr) {
		this.abbr = abbr;
	}
	public Set<SystemOption> getSystemOptions() {
		return systemOptions;
	}
	public void setSystemOptions(Set<SystemOption> systemOptions) {
		this.systemOptions = systemOptions;
	}
}