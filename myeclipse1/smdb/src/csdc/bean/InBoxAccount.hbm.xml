<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="csdc.bean.InBoxAccount" table="T_INBOX_ACCOUNT" >
        <id name="id" type="java.lang.String">
            <column name="C_ID" length="40" />
            <generator class="uuid.hex" />
        </id>
        
        
        <many-to-one name="inBoxId" class="csdc.bean.InBox" fetch="select">
            <column name="C_INBOX_ID" length="40"  />
        </many-to-one>
        
        <many-to-one name="recId" class="csdc.bean.Account" fetch="select">
            <column name="C_ACCOUNT_ID" length="40" />
        </many-to-one>
        
         <property name="status" type="java.lang.Integer">
        	<column name="C_STATUS" precision="1" scale="0" />
        </property>
        
        <property name="readTime" type="java.sql.Timestamp">
            <column name="C_READ_TIME " length="7"/>
        </property>
        
    </class>
</hibernate-mapping>