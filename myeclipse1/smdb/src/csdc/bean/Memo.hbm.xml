<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="csdc.bean.Memo" table="T_MEMO" >
        <id name="id" type="java.lang.String">
            <column name="C_ID" length="40" />
            <generator class="uuid.hex" />
        </id>
        <property name="title" type="java.lang.String">
            <column name="C_TITLE" length="200"/>
        </property>
        <property name="content" type="java.lang.String">
            <column name="C_CONTENT"/>
        </property>
        <property name="week" type="java.lang.String">
            <column name="C_WEEK" length="40"/>
        </property>
         <property name="month" type="java.lang.String">
            <column name="C_MONTH" length="40"/>
        </property>
        <property name="updateTime" type="java.sql.Timestamp">
            <column name="C_UPDATE_TIME" length="7"  />
        </property>
        <property name="remindTime" type="java.sql.Timestamp">
            <column name="C_REMIND_TIME" length="7"  />
        </property>
        <property name="startDateDay" type="java.sql.Timestamp">
            <column name="C_START_DATE_DAY" length="7"  />
        </property>
        <property name="endDateDay" type="java.sql.Timestamp">
            <column name="C_END_DATE_DAY" length="7"  />
        </property>
        <property name="startDateWeek" type="java.sql.Timestamp">
            <column name="C_START_DATE_WEEK" length="7"  />
        </property>
        <property name="endDateWeek" type="java.sql.Timestamp">
            <column name="C_END_DATE_WEEK" length="7"  />
        </property>
<!--        <property name="reverseRemindTime" type="java.sql.Timestamp">-->
<!--            <column name="C_REVERSE_REMINDTIME" length="7"  />-->
<!--        </property>-->
<!--        <property name="remindDay" type="java.lang.String">-->
<!--            <column name="C_REMINDDAY" length="40"  />-->
<!--        </property>-->
        <property name="excludeDate" type="java.lang.String">
            <column name="C_EXCLUDE_DATE" length="40"/>
        </property>
        <property name="isRemind" type="java.lang.Integer">
            <column name="C_IS_REMIND" precision="1" scale="0"/>
        </property>
         <property name="remindWay" type="java.lang.Integer">
            <column name="C_REMIND_WAY" precision="1" scale="0"/>
        </property>
       	<many-to-one name="account" class="csdc.bean.Account" fetch="select" lazy="false">
            <column name="C_ACCOUNT_ID" length="40" />
        </many-to-one>
    </class>
</hibernate-mapping>