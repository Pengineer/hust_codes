<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="csdc.bean.Job" table="T_JOB" >
        <id name="id" type="java.lang.String">
            <column name="C_ID" length="40" />
            <generator class="uuid.hex" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="C_NAME" />
        </property>
        <property name="number" type="java.lang.Integer">
            <column name="C_NUMBER" />
        </property>
        <property name="age" type="java.lang.String">
            <column name="C_AGE" />
        </property>
        <property name="degree" type="java.lang.String">
            <column name="C_DEGREE" />
        </property>
        <property name="requirement" type="java.lang.String">
            <column name="C_REQUIREMENT" />
        </property>
        <property name="publishDate" type="java.util.Date">
            <column name="C_PUBLISH_DATE" />
        </property>
        <property name="endDate" type="java.util.Date">
            <column name="C_END_DATE" />
        </property>
    </class>
</hibernate-mapping>