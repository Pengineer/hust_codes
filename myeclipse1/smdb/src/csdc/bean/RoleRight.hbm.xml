<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="csdc.bean.RoleRight" table="T_ROLE_RIGHT" >
        <id name="id" type="java.lang.String">
            <column name="C_ID" length="40" />
            <generator class="uuid.hex" />
        </id>
        <many-to-one name="role" class="csdc.bean.Role" fetch="select">
            <column name="C_ROLE_ID" length="40" not-null="true" />
        </many-to-one>
        <many-to-one name="right" class="csdc.bean.Right" fetch="select">
            <column name="C_RIGHT_ID" length="40" not-null="true" />
        </many-to-one>
    </class>
</hibernate-mapping>