<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="csdc.bean.AgencyFunding" table="T_AGENCY_FUNDING">
		<id name="id" type="java.lang.String">
			<column name="C_ID" length="40" />
			<generator class="uuid.hex" />
		</id>
		<property name="agencyName" type="java.lang.String">
			<column name="C_AGENCY_NAME" length="200" />
		</property>
		<many-to-one name="agency" class="csdc.bean.Agency" lazy="false">
			<column name="C_AGENCY_ID" length="40" />
		</many-to-one>		

		<property name="fee" type="java.lang.Double">
			<column name="C_FEE" not-null="true" />
		</property>
		<property name="count" type="java.lang.Integer">
        	<column name="C_COUNT" precision="1" scale="0" />
        </property>
		<many-to-one name="fundingBatch" class="csdc.bean.FundingBatch" lazy="false">
			<column name="C_FUNDING_BATCH_ID" length="40" />
		</many-to-one>
		<many-to-one name="bankAccount" class="csdc.bean.BankAccount" lazy="false">
			<column name="C_BANK_ACCOUNT_ID" length="40" />
		</many-to-one>
	</class>
</hibernate-mapping>
