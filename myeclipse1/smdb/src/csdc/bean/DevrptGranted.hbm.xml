<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<joined-subclass name="csdc.bean.DevrptGranted" extends="csdc.bean.ProjectGranted" table="T_DEVRPT_GRANTED">
		<key column="C_ID"/>
        <many-to-one name="application" class="csdc.bean.DevrptApplication" fetch="select">
            <column name="C_APPLICATION_ID" length="40" />
        </many-to-one>
        <set name="devrptAnninspection" inverse="true" cascade="all">
            <key>
                <column name="C_GRANTED_ID" length="40" not-null="true" />
            </key>
            <one-to-many class="csdc.bean.DevrptAnninspection" />
        </set>
        <set name="devrptVariation" inverse="true" cascade="all">
            <key>
                <column name="C_GRANTED_ID" length="40" not-null="true" />
            </key>
            <one-to-many class="csdc.bean.DevrptVariation" />
        </set>
        <set name="devrptMidinspection" inverse="true" cascade="all">
            <key>
                <column name="C_GRANTED_ID" length="40" not-null="true" />
            </key>
            <one-to-many class="csdc.bean.DevrptMidinspection" />
        </set>
        <set name="devrptEndinspection" inverse="true" cascade="all">
            <key>
                <column name="C_GRANTED_ID" length="40" not-null="true" />
            </key>
            <one-to-many class="csdc.bean.DevrptEndinspection" />
        </set>
        <set name="devrptFunding" inverse="true" cascade="all">
            <key>
                <column name="C_GRANTED_ID" length="40" not-null="true" />
            </key>
            <one-to-many class="csdc.bean.DevrptFunding" />
        </set>
	</joined-subclass>
</hibernate-mapping>
