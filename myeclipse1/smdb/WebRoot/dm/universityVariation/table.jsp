<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@ taglib prefix="s" uri="/struts-tags"%>
<div class="main">
	<div class="main_content">
		<table width="100%" border="0" cellspacing="2" cellpadding="0">
			<tr class="table_tr2">
				<td class="table_td2" width="120">原高校名称：</td>
				<td class="table_td3"><s:textfield id="nameOld" name="universityVariation.nameOld" cssClass="input_css" /></td>
				<td class="table_td4"></td>
			</tr>
			<tr class="table_tr2">
				<td class="table_td2" width="120">原高校代码：</td>
				<td class="table_td3"><s:textfield id="codeOld" name="universityVariation.codeOld" cssClass="input_css" /></td>
				<td class="table_td4"></td>
			</tr>
			<tr class="table_tr2">
				<td class="table_td2" width="120">新高校名称：</td>
				<td class="table_td3"><s:textfield id="nameNew" name="universityVariation.nameNew" cssClass="input_css" /></td>
				<td class="table_td4"></td>
			</tr>
			<tr class="table_tr2">
				<td class="table_td2" width="120">新高校代码：</td>
				<td class="table_td3"><s:textfield id="codeNew" name="universityVariation.codeNew" cssClass="input_css" /></td>
				<td class="table_td4"></td>
			</tr>
			<tr class="table_tr2">
				<td class="table_td2" width="120">变更时间：</td>
				<td class="table_td3"><s:textfield id="variationDate" name="universityVariation.variationDate" cssClass="input_css" /></td>
				<td class="table_td4"></td>
			</tr>
			<tr class="table_tr2">
				<td class="table_td2">类别：</td>
				<td class="table_td3">
					<s:select cssClass="select" name="universityVariation.type" headerKey="0" headerValue="--请选择--"
						list="#{'1':'更名','2':'合并'}" />
					<s:hidden name="entityId" id="universityVariation.id"/>
				</td>
				<td class="table_td4"></td>
			</tr>
			<tr class="table_tr2">
				<td class="table_td2" width="120">备注：</td>
				<td class="table_td3"><s:textfield id="description" name="universityVariation.description" cssClass="input_css" /></td>
				<td class="table_td4"></td>
			</tr>
			
		</table>
	</div> 
</div>


