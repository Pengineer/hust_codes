<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="csdc.bean.BankAccount" table="T_Bank_Account" >
		<id name="id" type="java.lang.String">
	            <column name="C_ID" length="40" />
	            <generator class="uuid.hex" />
	    </id>
	    
	    <many-to-one name="province" class="csdc.bean.SystemOption" fetch="select">
	            <column name="C_PROVINCE_ID" length="40"/>
	    </many-to-one>
	    <many-to-one name="city" class="csdc.bean.SystemOption" fetch="select">
	            <column name="C_city_ID" length="40"/>
	    </many-to-one>
	    <property name="ids" type="java.lang.String">
	            <column name="C_IDS" length="40" />
	    </property>
	    <property name="sn" type="java.lang.Integer">
	        <column name="C_SN"/>
	    </property>
	    <property name="bankName" type="java.lang.String">
	            <column name="C_BANK_NAME" length="200" />
	    </property>
	    <property name="bankCupNumber" type="java.lang.String">
	            <column name="C_BANK_CUP_NUMBER" length="90"/>
	    </property>
	    <property name="accountNumber" type="java.lang.String">
	            <column name="C_ACCOUNT_NUMBER" length="80" />
	    </property>
	    <property name="accountName" type="java.lang.String">
	            <column name="C_ACCOUNT_NAME" length="200" />
	    </property>
	    <property name="isDefault" type="java.lang.Integer">
	            <column name="C_IS_DEFAULT" precision="1" scale="0" />
	    </property>
	    <property name="createMode" type="java.lang.Integer">
	            <column name="C_CREATE_MODE" precision="1" scale="0" />
	    </property>
	    <property name="createDate" type="java.util.Date">
	            <column name="C_CREATE_DATE" length="7"/>
	    </property>
	    <property name="updateDate" type="java.util.Date">
	            <column name="C_UPDATE_DATE" length="7"/>
	    </property>
	</class>

</hibernate-mapping>
