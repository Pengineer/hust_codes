<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<joined-subclass name="csdc.bean.SpecialGranted" extends="csdc.bean.ProjectGranted" table="T_SPECIAL_GRANTED">
		<key column="C_ID"/>
        <many-to-one name="application" class="csdc.bean.SpecialApplication" fetch="select">
            <column name="C_APPLICATION_ID" length="40" />
        </many-to-one>
        <set name="specialAnninspection" inverse="true" cascade="all">
            <key>
                <column name="C_GRANTED_ID" length="40" not-null="true" />
            </key>
            <one-to-many class="csdc.bean.SpecialAnninspection" />
        </set>
        <set name="specialVariation" inverse="true" cascade="all">
            <key>
                <column name="C_GRANTED_ID" length="40" not-null="true" />
            </key>
            <one-to-many class="csdc.bean.SpecialVariation" />
        </set>
        <set name="specialMidinspection" inverse="true" cascade="all">
            <key>
                <column name="C_GRANTED_ID" length="40" not-null="true" />
            </key>
            <one-to-many class="csdc.bean.SpecialMidinspection" />
        </set>
        <set name="specialEndinspection" inverse="true" cascade="all">
            <key>
                <column name="C_GRANTED_ID" length="40" not-null="true" />
            </key>
            <one-to-many class="csdc.bean.SpecialEndinspection" />
        </set>
        <set name="specialFunding" inverse="true" cascade="all">
            <key>
                <column name="C_GRANTED_ID" length="40" not-null="true" />
            </key>
            <one-to-many class="csdc.bean.SpecialFunding" />
        </set>
	</joined-subclass>

</hibernate-mapping>
