<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="csdc.bean.ProjectData" table="T_PROJECT_DATA" >
        <id name="id" type="java.lang.String">
            <column name="C_ID" length="40" />
            <generator class="uuid.hex" />
        </id>
        <many-to-one name="projectType" class="csdc.bean.SystemOption" fetch="select">
            <column name="C_PROJECT_TYPE_ID" length="40" />
        </many-to-one>
        <many-to-one name="university" class="csdc.bean.Agency" fetch="select">
        	<column name="C_UNIVERSITY_ID" length="40" />
        </many-to-one>
        <many-to-one name="department" class="csdc.bean.Department" fetch="select">
        	<column name="C_DEPARTMENT_ID" length="40" />
        </many-to-one>
         <many-to-one name="institute" class="csdc.bean.Institute" fetch="select">
        	<column name="C_INSTITUTE_ID" length="40" />
        </many-to-one>
        <property name="granted" type="java.lang.String">
        	<column name="C_GRANTED_ID" length="40" />
        </property>
        <property name="endinspection" type="java.lang.String">
        	<column name="C_ENDINSPECTION_ID" length="40" />
        </property>
        <property name="file" type="java.lang.String">
        	<column name="C_FILE" length="100" />
        </property>
        <property name="keywords" type="java.lang.String">
        	<column name="C_KEYWORDS" length="400" />
        </property>
        <property name="summary" type="java.lang.String">
        	<column name="C_ABSTRACT" length="800" />
        </property>
        <property name="introduction" type="java.lang.String">
            <column name="C_INTRODUCTION" />
        </property>
        <property name="producerName" type="java.lang.String">
            <column name="C_PRODUCER_NAME" length="200" />
        </property>
        <property name="agencyName" type="java.lang.String">
            <column name="C_AGENCY_NAME" length="200" />
        </property>
        <property name="divisionName" type="java.lang.String">
            <column name="C_DIVISION_NAME" length="200" />
        </property>
        <property name="surveyMethod" type="java.lang.String">
            <column name="C_SURVEY_METHOD" length="200" />
        </property>
        <property name="surveyField" type="java.lang.String">
            <column name="C_SURVEY_FIELD" length="200" />
        </property>
        <property name="startDate" type="java.util.Date">
            <column name="C_START_DATE" length="7" />
        </property>
        <property name="endDate" type="java.util.Date">
            <column name="C_END_DATE" length="7" />
        </property>
        <property name="submitDate" type="java.util.Date">
            <column name="C_SUBMIT_DATE" length="7" />
        </property>
        <property name="note" type="java.lang.String">
            <column name="C_NOTE" length="800" />
        </property>
        <property name="dfs" type="java.lang.String">
            <column name="C_DFS" length="200" />
        </property>
    </class>
</hibernate-mapping>