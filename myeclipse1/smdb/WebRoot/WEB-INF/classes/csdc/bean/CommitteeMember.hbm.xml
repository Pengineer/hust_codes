<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="csdc.bean.CommitteeMember" table="T_COMMITTEE_MEMBER" >
        <id name="id" type="java.lang.String">
            <column name="C_ID" length="40" />
            <generator class="uuid.hex" />
        </id>
        <many-to-one name="committee" class="csdc.bean.Committee" fetch="select">
            <column name="C_COMMITTEE_ID" length="40" not-null="true" />
        </many-to-one>
        <many-to-one name="member" class="csdc.bean.Person" fetch="select">
            <column name="C_MEMBER_ID" length="40" not-null="true" />
        </many-to-one>
        <property name="position" type="java.lang.String">
            <column name="C_POSITION" length="80" />
        </property>
    </class>
</hibernate-mapping>