<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="nssf" extends="main" namespace="/other/nssf/granted">

		<action name="toList" class="nssfAction" method="toList">
			<result>/other/nssf/granted/list.jsp</result>
		</action>

		<action name="list" class="nssfAction" method="list">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>

		<action name="simpleSearch" class="nssfAction" method="simpleSearch">
			<result type="chain">list</result>
		</action>
		
		<action name="advSearch" class="nssfAction" method="advSearch">
			<result type="chain">list</result>
		</action>
		
		<action name="sort" class="nssfAction" method="sort">
			<result type="chain">list</result>
		</action>

		<action name="changePageSize" class="nssfAction" method="changePageSize">
			<result type="chain">list</result>
		</action>

		<action name="toPage" class="nssfAction" method="toPage">
			<result type="chain">list</result>
		</action>

		<action name="toViewTask" class="nssfAction" method="toViewTask">
			<result>/other/nssf/granted/viewTask.jsp</result>
		</action>

		<action name="viewTask" class="nssfAction" method="viewTask">
			<result type="json"><param name="root">jsonMap</param></result>
		</action>

		<action name="update" class="nssfAction" method="update">
			<result type="json"><param name="root">jsonMap</param></result>
		</action>
			<!-- 导出 -->
	    <action name="confirmExportOverView" class="nssfAction" method="confirmExportOverView">
        	<result name="success" type="stream">
				<param name="contentType">application/vnd.ms-excel</param>
				<param name="contentDisposition">attachment;fileName="${fileFileName}"</param>
				<param name="inputName">downloadFile</param>
			</result>
	   	</action>
		

	</package>
	
	<package name="nssfApplication" extends="main" namespace="/other/nssf/application">

		<action name="toList" class="nssfApplicationAction" method="toList">
			<result>/other/nssf/application/list.jsp</result>
		</action>

		<action name="list" class="nssfApplicationAction" method="list">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>

		<action name="simpleSearch" class="nssfApplicationAction" method="simpleSearch">
			<result type="chain">list</result>
		</action>
		
		<action name="sort" class="nssfApplicationAction" method="sort">
			<result type="chain">list</result>
		</action>

		<action name="changePageSize" class="nssfApplicationAction" method="changePageSize">
			<result type="chain">list</result>
		</action>

		<action name="toPage" class="nssfApplicationAction" method="toPage">
			<result type="chain">list</result>
		</action>

		<!-- 进入初审页面 -->
		<action name="toFirstAudit" class="nssfApplicationAction" method="toFirstAudit">
			<result>/other/nssf/application/firstAuditConfig.jsp</result>
		</action>
		
		<action name="firstAudit" class="nssfApplicationAction" method="firstAudit">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		
		<!-- 导出 -->
	    <action name="confirmExportOverView" class="nssfApplicationAction" method="confirmExportOverView">
        	<result name="success" type="stream">
				<param name="contentType">application/vnd.ms-excel</param>
				<param name="contentDisposition">attachment;fileName="${fileFileName}"</param>
				<param name="inputName">downloadFile</param>
			</result>
	   	</action>
	   	
	</package>

</struts>
