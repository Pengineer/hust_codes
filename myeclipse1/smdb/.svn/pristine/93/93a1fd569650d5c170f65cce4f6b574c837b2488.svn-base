<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="csdc.bean.FundingList" table="T_FUNDING_LIST" >
        <id name="id" type="java.lang.String">
            <column name="C_ID" length="40" />
            <generator class="uuid.hex" />
        </id>
        <property name="name" type="java.lang.String">
        	<column name="C_NAME" length="200" />
        </property>
        <property name="attn" type="java.lang.String">
        	<column name="C_ATTN" length="60" />
        </property>
        <property name="createDate" type="java.util.Date">
            <column name="C_CREATE_DATE" length="7" />
        </property>                
        <property name="status" type="java.lang.Integer">
        	<column name="C_STATUS" precision="1" scale="0" />
        </property>     
        <property name="note" type="java.lang.String">
        	<column name="C_NOTE" length="200" />
        </property>        
        <property name="type" type="java.lang.String">
        	<column name="C_TYPE" length="60" />
        </property>         
        <property name="subType" type="java.lang.String">
        	<column name="C_SUB_TYPE" length="40" />
        </property>        
        <property name="subSubType" type="java.lang.String">
        	<column name="C_Sub_Sub_Type" length="40" />
        </property>         
        <property name="rate" type="java.lang.Double">
            <column name="C_RATE" precision="5" scale="2" />
        </property>
        <property name="year" type="java.lang.Integer">
        	<column name="C_YEAR" precision="1" scale="0" />
        </property>                
        <many-to-one name="fundingBatch" class="csdc.bean.FundingBatch" fetch="select">
            <column name="C_FUNDING_BATCH_ID" length="40" not-null="true" />
        </many-to-one>
    </class>
</hibernate-mapping>