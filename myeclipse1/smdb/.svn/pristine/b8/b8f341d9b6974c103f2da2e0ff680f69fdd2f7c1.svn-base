<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
    <joined-subclass name="csdc.bean.Patent" extends="csdc.bean.Product" table="T_PATENT">
    	<key column="C_ID"/>
        <property name="applicationNumber" type="java.lang.String">
            <column name="C_APPLICATION_NUMBER" length="40" />
        </property>
        <property name="applicationDate" type="java.util.Date">
            <column name="C_APPLICATION_DATE" length="7" />
        </property>
        <property name="publicNumber" type="java.lang.String">
            <column name="C_PUBLIC_NUMBER" length="40" />
        </property>
        <property name="publicDate" type="java.util.Date">
            <column name="C_PUBLIC_DATE" length="7" />
        </property> 
        <property name="classNumber" type="java.lang.String">
            <column name="C_CLASS_NUMBER" length="40" />
        </property>
        <property name="priorityNumber" type="java.lang.String">
            <column name="C_PRIORITY_NUMBER" length="40" />
        </property> 
        <property name="inventorName" type="java.lang.String">
            <column name="C_INVENTOR_NAME" length="400" />
        </property>
        <property name="independentClaims" type="java.lang.String">
            <column name="C_INDEPENDENT_CLAIMS" length="800" />
        </property>
       	<property name="categotyType" type="java.lang.String">
            <column name="C_CATEGOTY_TYPE" length="40" />
        </property>
        <property name="summary" type="java.lang.String">
            <column name="C_SUMMARY" length="800" />
        </property>
        <property name="countriesProvinceCode" type="java.lang.String">
            <column name="C_COUNTRIES_PROVINCE_CODE" length="40" />
        </property> 
        <property name="patentScope" type="java.lang.String">
            <column name="C_PATENT_SCOPE" length="40" />
        </property> 
        
    </joined-subclass>
    
</hibernate-mapping>