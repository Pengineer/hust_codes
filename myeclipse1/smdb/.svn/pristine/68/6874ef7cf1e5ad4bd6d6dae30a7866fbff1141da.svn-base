<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="csdc.bean.SinossBook" table="T_SINOSS_BOOK">
        <id name="id" type="java.lang.String">
            <column name="C_ID" length="40" />
            <generator class="uuid.hex" />
        </id>
        <property name="sinossId" type="java.lang.String">
            <column name="C_SINOSS_ID"/>
        </property>
	    <property name="projectName" type="java.lang.String">
            <column name="C_PROJECT_NAME"/>
        </property>
        <property name="dumpDate" type="java.util.Date">
			<column name="C_DUMP_DATE" />
		</property>      
	    <property name="press" type="java.lang.String">
            <column name="C_PRESS"/>
        </property> 
	    <property name="bookType" type="java.lang.String">
            <column name="C_BOOK_TYPE"/>
        </property>
        <property name="pressTime" type="java.util.Date">
            <column name="C_PRESS_TIME" />
        </property>		
        <property name="isEnglish" type="java.lang.String">
            <column name="C_IS_ENGLISH" />
        </property>
	    <property name="pressAddress" type="java.lang.String">
            <column name="C_PRESS_ADDRESS"/>
        </property>
 	    <property name="name" type="java.lang.String">
            <column name="C_NAME"/>
        </property>		
 	    <property name="firstAuthor" type="java.lang.String">
            <column name="C_FIRST_AUTHOR"/>
        </property>
 	    <property name="otherAuthor" type="java.lang.String">
            <column name="C_OTHER_AUTHOR"/>
        </property>
 	    <property name="subject" type="java.lang.String">
            <column name="C_SUBJECT"/>
        </property>	
 	    <property name="disciplineType" type="java.lang.String">
            <column name="C_DISCIPLINE_TYPE"/>
        </property>	
        <property name="isMark" type="java.lang.String">
            <column name="C_IS_MARK" />
        </property>	
 	    <property name="isbn" type="java.lang.String">
            <column name="C_ISBN"/>
        </property>	
 	    <property name="cip" type="java.lang.String">
            <column name="C_CIP"/>
        </property>		
 	    <property name="note" type="java.lang.String">
            <column name="C_NOTE"/>
        </property>	
 	    <property name="projectId" type="java.lang.String">
            <column name="C_PROJECT_ID"/>
        </property>	
    	<property name="wordNumber" type="java.lang.String">
            <column name="C_WORD_NUMBER"/>
        </property>
        <many-to-one name="projectMidinspection" class="csdc.bean.SinossProjectMidinspection" fetch="join">
            <column name="C_project_Midinspection_ID"/>
        </many-to-one>			
    </class>
</hibernate-mapping>