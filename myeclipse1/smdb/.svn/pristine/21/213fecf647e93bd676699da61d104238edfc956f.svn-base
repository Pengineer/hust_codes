<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<joined-subclass name="csdc.bean.EntrustGranted" extends="csdc.bean.ProjectGranted" table="T_ENTRUST_GRANTED">
		<key column="C_ID"/>
        <many-to-one name="application" class="csdc.bean.EntrustApplication" fetch="select">
            <column name="C_APPLICATION_ID" length="40" />
        </many-to-one>
        <set name="entrustVariation" inverse="true" cascade="all">
            <key>
                <column name="C_GRANTED_ID" length="40" not-null="true" />
            </key>
            <one-to-many class="csdc.bean.EntrustVariation" />
        </set>
        <set name="entrustEndinspection" inverse="true" cascade="all">
            <key>
                <column name="C_GRANTED_ID" length="40" not-null="true" />
            </key>
            <one-to-many class="csdc.bean.EntrustEndinspection" />
        </set>
        <set name="entrustFunding" inverse="true" cascade="all">
            <key>
                <column name="C_GRANTED_ID" length="40" not-null="true" />
            </key>
            <one-to-many class="csdc.bean.EntrustFunding" />
        </set>
	</joined-subclass>

</hibernate-mapping>
