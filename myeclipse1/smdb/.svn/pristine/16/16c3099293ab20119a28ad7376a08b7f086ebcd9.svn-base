package csdc.tool.filter;

import java.io.File;
import java.io.FileFilter;

/**
 * @author yingao
 * 文件过滤器，判断是否是文件
 */
public class FileFilterTool implements FileFilter {

	@Override
	public boolean accept(File pathname) {
        if (pathname.isFile()) {
            return true;
        }
		return false;
	}

}
