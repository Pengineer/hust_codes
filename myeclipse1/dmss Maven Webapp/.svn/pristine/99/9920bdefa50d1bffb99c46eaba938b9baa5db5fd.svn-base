package org.csdc.domain.sm.hdfs;

/**
 * HDFS线程管理表单
 * @author jintf
 * @date 2014-6-15
 */
@Deprecated
public class ThreadForm {
	private Integer tempFileCount;
	private Integer tempFileSize;
	private Integer totalBlockCount; //总块数
	private Integer tempBlockCount; //临时块数
	private Double tempPercent; //临时块占比
	private Integer blockSize;
	private Integer period; //块合并周期
	private Integer garbageCount;
	private Double garbagePercent;
	
	public Integer getTempFileCount() {
		return tempFileCount;
	}
	public void setTempFileCount(Integer tempFileCount) {
		this.tempFileCount = tempFileCount;
	}
	public Integer getTempFileSize() {
		return tempFileSize;
	}
	public void setTempFileSize(Integer tempFileSize) {
		this.tempFileSize = tempFileSize;
	}
	public Integer getTotalBlockCount() {
		return totalBlockCount;
	}
	public void setTotalBlockCount(Integer totalBlockCount) {
		this.totalBlockCount = totalBlockCount;
	}
	public Integer getTempBlockCount() {
		return tempBlockCount;
	}
	public void setTempBlockCount(Integer tempBlockCount) {
		this.tempBlockCount = tempBlockCount;
	}
	public Double getTempPercent() {
		return tempPercent;
	}
	public void setTempPercent(Double tempPercent) {
		this.tempPercent = tempPercent;
	}
	public Integer getBlockSize() {
		return blockSize;
	}
	public void setBlockSize(Integer blockSize) {
		this.blockSize = blockSize;
	}
	public Integer getPeriod() {
		return period;
	}
	public void setPeriod(Integer period) {
		this.period = period;
	}
	public Integer getGarbageCount() {
		return garbageCount;
	}
	public void setGarbageCount(Integer garbageCount) {
		this.garbageCount = garbageCount;
	}
	public Double getGarbagePercent() {
		return garbagePercent;
	}
	public void setGarbagePercent(Double garbagePercent) {
		this.garbagePercent = garbagePercent;
	}
	
}
