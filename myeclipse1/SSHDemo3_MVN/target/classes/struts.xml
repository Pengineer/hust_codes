<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC 
      "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
      "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="register" namespace="/register" extends="json-default">
		<action name="addUser" class="registerAction" method="addUser">
			<result name="success">/RegisterSuccess.jsp</result>
			<result name="fail">/RegisterFail.jsp</result>
		</action>
		
		<action name="deleteUser">
			<result></result>
		</action>
	</package>
	
	<package name="register1" namespace="/list" extends="json-default">
		<action name="list" class="userAction" method="listUsers">
			<result>/list.jsp</result>
		</action>
	</package>

</struts>