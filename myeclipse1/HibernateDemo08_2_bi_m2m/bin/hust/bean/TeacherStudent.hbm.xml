<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hust.bean">
	<class name="hust.bean.TeacherStudent" table="t_teacher_student">
		<id name="id" column="c_id">
			<generator class="increment" />
		</id>
		
		<many-to-one name="teacher" class="hust.bean.Teacher" >
			<column name="c_teacher_id" not-null="true"/>
		</many-to-one>
		
		<many-to-one name="student" class="hust.bean.Student" >
			<column name="c_student_id" not-null="true"/>
		</many-to-one>
		
		<property name="createDate" type="java.util.Date" column="c_create_date" />
	</class>
</hibernate-mapping>