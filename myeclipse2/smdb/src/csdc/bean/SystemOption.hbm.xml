        	</key>
        	<one-to-many class="csdc.bean.News" />
        </set>
        <set name="noticeType" inverse="true">
        	<key>
        		<column name="C_TYPE_ID" length="40" />
        	</key>
        	<one-to-many class="csdc.bean.Notice" />
        </set>
        <set name="messageType" inverse="true">
        	<key>
        		<column name="C_TYPE_ID" length="40" not-null="true" />
        	</key>
        	<one-to-many class="csdc.bean.Message" />
        </set>
        <set name="expertType" inverse="true">
        	<key>
        		<column name="C_EXPERT_TYPE_ID" length="40" />
        	</key>
