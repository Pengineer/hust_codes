        	</key>
        	<one-to-many class="csdc.bean.Electronic" />
        </set>
        <set name="bookType" inverse="true">
        	<key>
        		<column name="C_TYPE_ID" length="40" />
        	</key>
        	<one-to-many class="csdc.bean.Book" />
        </set>
        <set name="subtype" inverse="true">
        	<key>
        		<column name="C_SUBTYPE_ID" length="40" not-null="true" />
        	</key>
        	<one-to-many class="csdc.bean.GeneralApplication" />
        </set>
        <set name="topic" inverse="true">
        	<key>
        		<column name="C_TOPIC_ID" length="40" />
        	</key>
