<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="csdc.bean.DMResult" table="DM_RESULT" >
        <id name="id" type="java.lang.String">
            <column name="C_ID" length="40" />
            <generator class="uuid.hex" />
        </id>
        <property name="title" type="java.lang.String">
            <column name="C_TITLE" />
        </property>
        <property name="description" type="java.lang.String">
            <column name="C_DESCRIPTION" />
        </property>
        <property name="type" type="java.lang.String">
            <column name="C_TYPE" />
        </property>
        <property name="date" type="java.util.Date">
            <column name="C_DATE" length="7" />
        </property>
        <property name="config" type="java.lang.String">
            <column name="C_CONFIG" />
        </property>
        <property name="resultJson" type="java.lang.String">
            <column name="C_RESULT_JSON" />
        </property>
    </class>
</hibernate-mapping>