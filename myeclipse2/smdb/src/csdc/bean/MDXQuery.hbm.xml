<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="csdc.bean.MDXQuery" table="S_MDX_QUERY" >
        <id name="id" type="java.lang.String">
            <column name="C_ID" length="40" />
            <generator class="uuid.hex" />
        </id>
        <property name="title" type="java.lang.String">
            <column name="C_TITLE" />
        </property>
        <property name="description" type="java.lang.String">
            <column name="C_DESCRIPTION" />
        </property>
        <property name="mdx" type="java.lang.String">
            <column name="C_MDX" />
        </property>
        <property name="type" type="java.lang.String">
            <column name="C_TYPE" />
        </property>
        <property name="year" type="java.lang.Integer">
            <column name="C_YEAR" precision="4" scale="0" />
        </property>
        <property name="date" type="java.util.Date">
            <column name="C_DATE" length="7" />
        </property>
        <property name="chartConfig" type="java.lang.String">
            <column name="C_CHART_CONFIG" />
        </property>
        <property name="chartJson" type="java.lang.String">
            <column name="C_CHART_JSON" />
        </property>
        <property name="isHomeShow" type="java.lang.Integer">
            <column name="C_IS_HOME_SHOW" />
        </property>
    </class>
</hibernate-mapping>