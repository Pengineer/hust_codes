<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<package name="universityVariation" extends="main" namespace="/dm/universityVariation">

		<action name="toAdd" class="universityVariationAction" method="toAdd">
			<result>/dm/universityVariation/add.jsp</result>
		</action>

		<action name="add" class="universityVariationAction" method="add">
			<result type="redirectAction">
				<param name="actionName">toView</param>
				<param name="entityId">${entityId}</param>
			</result>
			<result name="input">/dm/universityVariation/add.jsp</result>
		</action>

		<action name="toView" class="universityVariationAction" method="toView">
			<result>/dm/universityVariation/view.jsp</result>
		</action>
		
		<action name="view" class="universityVariationAction" method="view">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		
		<action name="toModify" class="universityVariationAction" method="toModify">
			<result>/dm/universityVariation/modify.jsp</result>
		</action>

		<action name="modify" class="universityVariationAction" method="modify">
			<result type="redirectAction">
				<param name="actionName">toView</param>
				<param name="entityId">${entityId}</param>
				<param name="viewFlag">${viewFlag}</param>
				<param name="update">1</param>
			</result>
			<result name="input">/dm/universityVariation/modify.jsp</result>
		</action>
		
		<action name="delete" class="universityVariationAction" method="delete">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		
		<action name="toList" class="universityVariationAction" method="toList">
			<result>/dm/universityVariation/list.jsp</result>
		</action>
		
		<action name="list" class="universityVariationAction" method="list">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		
		<action name="simpleSearch" class="universityVariationAction" method="simpleSearch">
			<result type="chain">list</result>
		</action>
	
		<action name="changePageSize" class="universityVariationAction" method="changePageSize">
			<result type="chain">list</result>
		</action>
		
		<action name="sort" class="universityVariationAction" method="sort">
			<result type="chain">list</result>
		</action>
		
		<action name="prev" class="universityVariationAction" method="prev">
			<result type="redirectAction">
				<param name="actionName">view</param>
				<param name="entityId">${entityId}</param>
			</result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		
		<action name="next" class="universityVariationAction" method="next">
			<result type="redirectAction">
				<param name="actionName">view</param>
				<param name="entityId">${entityId}</param>
			</result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>

		<action name="toPage" class="universityVariationAction" method="toPage">
			<result type="chain">list</result>
		</action>
		
	</package>
	
</struts>