<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="KeyFund" extends="main" namespace="/projectFund/key">
	
		<action name="toList" class="KeyFundAction" method="toList">
			<result>/projectFund/keyFund/list.jsp</result>
		</action>

		<action name="list" class="KeyFundAction" method="list">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		
		<action name="simpleSearch" class="KeyFundAction" method="simpleSearch">
			<result type="chain">list</result>
		</action>
		
		<action name="advSearch" class="KeyFundAction" method="advSearch">
			<result type="chain">list</result>
		</action>
		
		<action name="sort" class="KeyFundAction" method="sort">
			<result type="chain">list</result>
		</action>

		<action name="changePageSize" class="KeyFundAction" method="changePageSize">
			<result type="chain">list</result>
		</action>

		<action name="toPage" class="KeyFundAction" method="toPage">
			<result type="chain">list</result>
		</action>
		
		<!-- 进入查看页面 -->
		<action name="toView" class="KeyFundAction" method="toView">
	        <result>/projectFund/keyFund/view.jsp</result>
	    </action>
	    
	    <!-- 查看 -->
	    <action name="view" class="KeyFundAction" method="view">
	    	<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
	    </action>
	    
	    <!-- 导出 -->
	    <action name="confirmExportOverView" class="KeyFundAction" method="confirmExportOverView">
        	<result name="success" type="stream">
				<param name="contentType">application/vnd.ms-excel</param>
				<param name="contentDisposition">attachment;fileName="${fileFileName}"</param>
				<param name="inputName">downloadFile</param>
			</result>
	   	</action>
	   	
	</package>
</struts>