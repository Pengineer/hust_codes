.input_css_out {border:solid 1px #aaa; background: #FFF; height:18px; line-height:18px; width:99%; font-size:12px;}
.table_title9 {background:url(../image/ico04.gif) no-repeat left 3px; padding-left:15px; width:70px; display:inline-block;}//必填项

