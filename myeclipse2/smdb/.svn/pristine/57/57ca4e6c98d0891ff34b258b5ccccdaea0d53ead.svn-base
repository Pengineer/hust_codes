body {background:url(../image/login_bg.gif) repeat-x #C7BAD4; _margin-top:82px;}

li {margin-top:12px; height:25px; overflow:hidden;}
li span {margin-left:10px; float:left;}
li img {float:left;}

.login_box {margin:82px auto 0px auto; width:694px; height:440px; _margin-top:0px;}
.login_input_box {background:url(../image/login_02.jpg) no-repeat; height:290px;}
.login_input_area {padding-top:24px; *padding-top:36px; _padding-top:36px; margin-left:270px; height:234px;}
.login_input_area ul li {line-height:20px; *line-height:22px; _line-height:22px; margin-top:10px; *margin-top:6px; _margin-top:6px;}
.login_input_area ul li img {margin-top:3px;}
.login_input {width:177px; height:19px; _height:19px; border:solid 1px #aaaaaa; background:#FFF;}
.login_btn {background:url(../image/btn_bg01.gif) no-repeat; width:79px; height:23px; border:none;}
.login_btn_box {margin-top:20px; *margin-top:14px; _margin-top:14px; margin-left:54px;}
.login_btn_box1 {margin-top:14px; margin-left:70px;}
.login_btn_box2 {margin-top:14px; width:388px; text-align:center;}
.login_btn_box input, .login_btn_box1 input, .login_btn_box2 input {cursor:pointer; margin-left:15px; *padding-top:3px; _padding-top:3px;}

.login_footer {width:694px; margin:0px auto; height:80px; background:url(../image/login_footer.gif) no-repeat;}

.logn_ftxt {color:#a5a5a5; height:44px; padding-top:10px; padding-left:58px; padding-right:8px; background:url(../image/s_logo.png) no-repeat left center; margin-left:110px;_margin-left:55px; margin-top:28px; float:left;}
.login_img {background:url(../image/login_01.png) no-repeat; height:150px;}
.login_img .ico {margin:46px 11px 0 51px;}
.login_img .txt {margin:48px 0 0;}
.login_select_server_title {margin-top:12px; color:#454e75; border-bottom:solid 1px #7d7d7d; width:362px; padding-left:26px; background:url(../image/input_ico4.gif) no-repeat left center; height:25px; line-height:25px;}
.login_select_server_txt {color:#7d7d7d; margin-top:10px; padding-left:26px; *padding-left:23px; _padding-left:23px; width:362px;}
.login_find_pw_title {margin-top:6px; color:#454e75; border-bottom:solid 1px #7d7d7d; width:362px; padding-left:26px; background:url(../image/find_ico.gif) no-repeat left center; height:25px; line-height:25px;}
.login_find_pw_title1 {margin-top: 5px; *margin-top:8px; color: rgb(69, 78, 117);}
.login_find_pw_txt {color:#7d7d7d; line-height:30px; margin-top:10px; padding-left:26px;}
.login_error_title {margin-top:12px; color:#454e75; border-bottom:solid 1px #7d7d7d; width:362px; padding-left:26px; background:url(../image/error_ico.png) no-repeat left center; height:25px; line-height:25px;}
.login_reset_pw {padding-left:0px; margin-top:20px;}
.pw_title {display:inline-block; width:65px; text-align:right;}
.pw_error{color:#cb2c07;}

span.error {color:#FF7400; display:inline-block; padding-left:5px;}
div.error {color:#FF7400;}

.register_title {margin-top:6px; color:#454e75; border-bottom:solid 1px #7d7d7d; width:362px; padding-left:26px; background:url(../image/spacemanage.gif) no-repeat left center; height:25px; line-height:25px;}
.reg_title {display:inline-block; width:65px; text-align:right;}
.reg_required {background:url(../image/ico04.gif) no-repeat left 3px; padding-left:15px; width:50px; display:inline-block; text-align:right;}
.reg_success {display:inline-block; text-align:right; padding-left:75px; }

/******* 弹出层 ******/
.popup_win {background:url(../image/t_bg.png); padding:10px;}
.popup_win_box {background:#FFF; border:solid 1px #89809d; width:100%;height:100%;}
.popup_title {height:30px; background:url(../image/table_bg06.gif); cursor:default;}
.popup_title span {float:left; margin-left:12px; margin-top:6px; color:#FFF; font-size:14px; font-weight:bold;}
.popup_title #close {width:20px; height:20px; float:right; cursor:pointer;}
.popup_title .close_out {width:22px; height:22px; float:right; display:inline; cursor:pointer; margin-right:5px;margin-top:4px;}
.popup_title .close_out img {margin-top:7px; margin-left:7px;}
.popup_title .close_over {width:20px; height:20px; float:right; display:inline; cursor:pointer; border:1px dashed #FFFFFF;margin-right:5px;margin-top:4px;}
.popup_title .close_over img {margin-top:6px; margin-left:6px;}
.popup_content {padding:10px;}
.import_css{background:#A6FFA6;}
.head_title{background:#FFD0FF;height:36px;}
.head_title td {line-height:15px;text-align:center;}

