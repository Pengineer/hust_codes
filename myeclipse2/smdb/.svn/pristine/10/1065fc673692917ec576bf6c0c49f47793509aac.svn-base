<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@ taglib prefix="s" uri="/struts-tags"%>
<div class="main">
	<div class="main_content">
		<table width="100%" border="0" cellspacing="2" cellpadding="0">
			<tr class="table_tr2">
				<td class="table_td2" width="120"><s:text name="i18n_NameOld" />：</td>
				<td class="table_td3"><s:textfield id="nameOld" name="universityVariation.nameOld" cssClass="input_css" /></td>
				<td class="table_td4"></td>
			</tr>
			<tr class="table_tr2">
				<td class="table_td2" width="120"><s:text name="i18n_CodeOld" />：</td>
				<td class="table_td3"><s:textfield id="codeOld" name="universityVariation.codeOld" cssClass="input_css" /></td>
				<td class="table_td4"></td>
			</tr>
			<tr class="table_tr2">
				<td class="table_td2" width="120"><s:text name="i18n_NameNew" />：</td>
				<td class="table_td3"><s:textfield id="nameNew" name="universityVariation.nameNew" cssClass="input_css" /></td>
				<td class="table_td4"></td>
			</tr>
			<tr class="table_tr2">
				<td class="table_td2" width="120"><s:text name="i18n_CodeNew" />：</td>
				<td class="table_td3"><s:textfield id="codeNew" name="universityVariation.codeNew" cssClass="input_css" /></td>
				<td class="table_td4"></td>
			</tr>
			<tr class="table_tr2">
				<td class="table_td2"><s:text name="i18n_UrType" />：</td>
				<td class="table_td3">
					<s:select cssClass="select" name="universityVariation.type" headerKey="0" headerValue="--%{getText('i18n_PleaseSelect')}--"
						list="#{'1':getText('i18n_Rename'),'2':getText('i18n_Merge')}" />
					<s:hidden name="entityId" id="universityVariation.id"/>
				</td>
				<td class="table_td4"></td>
			</tr>
			<tr class="table_tr2">
				<td class="table_td2" width="120"><s:text name="i18n_Description" />：</td>
				<td class="table_td3"><s:textfield id="description" name="universityVariation.description" cssClass="input_css" /></td>
				<td class="table_td4"></td>
			</tr>
		</table>
	</div> 
</div>


