package csdc.action.unit;

import java.util.List;

public abstract class AgencyAction extends UnitAction{
	protected List agencies; // 管理机构列表对象
	protected String ids;// 管理机构编号序列
	
	public List getAgencies() {
		return agencies;
	}
	public void setAgencies(List agencies) {
		this.agencies = agencies;
	}
	public String getIds() {
		return ids;
	}
	public void setIds(String ids) {
		this.ids = ids;
	}
	
	
}
