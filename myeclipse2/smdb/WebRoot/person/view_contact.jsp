<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@ taglib prefix="s" uri="/struts-tags"%>
<%@ page isELIgnored ="true"%>

<div id="contact">
	<textarea class="view_template" style="display:none;">
	<div class="p_box_body">
		<div class="p_box_body_t">
			<table width="100%" border="1" cellspacing="0" cellpadding="0" bordercolor="#CCCCCC" style="border-collapse:collapse;">
				<tr class="table_tr7">
					<td class="key">住宅地址：</td>
					<td class="value" colspan="3">${person.homeAddress}</td>
				</tr>
				<tr class="table_tr7">
					<td class="key" width="100">住宅邮编：</td>
					<td class="value" width="200">${person.homePostcode}</td>
					<td class="key" width="100">住宅电话：</td>
					<td class="value">${person.homePhone}</td>
				</tr>
				<tr class="table_tr7">
					<td class="key">MSN：</td>
					<td class="value">${person.msn}</td>
					<td class="key" width="100">住宅传真：</td>
					<td class="value">${person.homeFax}</td>
				</tr>
				<tr class="table_tr7">
					<td class="key">办公地址：</td>
					<td class="value" colspan="3">${person.officeAddress}</td>
				</tr>
				<tr class="table_tr7">
					<td class="key">办公邮编：</td>
					<td class="value">${person.officePostcode}</td>
					<td class="key">办公电话：</td>
					<td class="value">${person.officePhone}</td>
				</tr>
				<tr class="table_tr7">
					<td class="key">办公传真：</td>
					<td class="value">${person.officeFax}</td>
					<td class="key">手机：</td>
					<td class="value">${person.mobilePhone}</td>
				</tr>
				<tr class="table_tr7">
					<td class="key">邮箱：</td>
					<td class="value"><a href="mailto:${person.email}">${person.email}</a></td>
					<td class="key">QQ：</td>
					<td class="value">${person.qq}</td>
				</tr>
				<tr class="table_tr7">
					<td class="key">个人主页：</td>
					<td class="value" colspan="3">{if person.homepage != null}<a href="${person.homepage}" target="_blank">${person.homepage}</a>{else}无{/if}</td>
				</tr>
			</table>
		</div>
	</div>
	</textarea>
</div>