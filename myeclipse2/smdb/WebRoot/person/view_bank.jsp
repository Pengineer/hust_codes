<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@ taglib prefix="s" uri="/struts-tags"%>
<%@ page isELIgnored ="true"%>

<div id="bank">
	<textarea class="view_template" style="display:none;">
	<div class="p_box_body">
		<div class="p_box_body_t">
			<table width="100%" border="1" cellspacing="0" cellpadding="0" bordercolor="#CCCCCC" style="border-collapse:collapse;">
				<tr class="table_tr7">
					<td class="key" width="100">开户银行：</td>
					<td class="value" width="200">${person.bankName}</td>
					<td class="key" width="100">银行支行：</td>
					<td class="value">${person.bankBranch}</td>
				</tr>
				<tr class="table_tr7">
					<td class="key">银联行号：</td>
					<td class="value" colspan="3">${person.cupNumber}</td>
				</tr>
				<tr class="table_tr7">
					<td class="key">银行户名：</td>
					<td class="value">${person.bankAccountName}</td>
					<td class="key">银行账号：</td>
					<td class="value">${person.bankAccount}</td>
				</tr>
			</table>
		</div>
	</div>
	</textarea>
</div>