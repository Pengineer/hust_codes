	        <result>/project/key/midinspection/applyAudit/popAdd.jsp</result>
	    </action> 
	    <action name="add" class="keyMidinspectionApplyAuditAction" method="add">
	   		<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
	    <action name="toModify" class="keyMidinspectionApplyAuditAction" method="toModify">
	        <result>/project/key/midinspection/applyAudit/popModify.jsp</result>
	    </action>
	    <action name="modify" class="keyMidinspectionApplyAuditAction" method="modify">
	   		<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		<action name="submit" class="keyMidinspectionApplyAuditAction" method="submit">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		<action name="back" class="keyMidinspectionApplyAuditAction" method="backAudit">
			<result type="json"><param name="root">jsonMap</param></result>
