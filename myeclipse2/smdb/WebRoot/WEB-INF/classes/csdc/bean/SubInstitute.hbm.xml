<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="csdc.bean.SubInstitute" table="T_SUB_INSTITUTE" >
        <id name="id" type="java.lang.String">
            <column name="C_ID" length="40" />
            <generator class="uuid.hex" />
        </id>
        <many-to-one name="director" class="csdc.bean.Person" fetch="select">
            <column name="C_DIRECTOR_ID" length="40" />
        </many-to-one>
        <many-to-one name="institute" class="csdc.bean.Institute" fetch="select">
            <column name="C_INSTITUTE_ID" length="40" not-null="true" />
        </many-to-one>
        <property name="name" type="java.lang.String">
            <column name="C_NAME" length="200" not-null="true" />
        </property>
        <property name="researchField" type="java.lang.String">
            <column name="C_RESEARCH_FIELD" length="2000" />
        </property>
    </class>
</hibernate-mapping>